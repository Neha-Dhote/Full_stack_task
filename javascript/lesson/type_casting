//number - >string
number - >boolean

//string - > number
string - >boolean            

//boolean - > number
boolean - > string

let data= true

console.log("data is :"+ data+ "its data type is:"+ type of(data))

// number to string

data =boolean(data)

if number is o than false else true

// string to number
"123a" - nan

data = number(data)

// string to boolean
if we have an empty string / false as a string| boolean value would be false else true
data = boolean(data)

// boolean to number
false - 0 true - 1
data = number(data)

// boolean to string
true - "true", false - "false"
data = string(data)

